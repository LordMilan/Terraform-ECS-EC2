[
    {
      "name": "${container_name}",
      "image": "${ecr_image}",
      "cpu": "${CPU}",
      "memoryReservation": "${memoryReservation}",
      "essential": "${essential}",
      "portMappings": [
        {
          "containerPort": "${containerPort}"
        }
      ],
      "logConfiguration": {
        "logDriver": "${log_driver}",
        "options": {
          "awslogs-group" : "${awslogs-group}",
          "awslogs-region": "${awslogs-region}"
        }
      }
    }
]
